{
  "name": "magebean/magebean-cli",
  "description": "Magebean CLI - Magento 2 AppSec Audit",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/console": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Magebean\\": "src/"
    }
  },
  "bin": ["bin/magebean"],
  "require-dev": {
    "humbug/box": "^4.6"
  },
  "scripts": {
    "build:phar": [
      "composer install",
      "php -d phar.readonly=0 vendor/bin/box compile"
    ],
    "build:clean": [
      "rm -f magebean.phar"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  }
}

